<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="._9_Manage_Tenants"
    android:id="@+id/page_layout_9">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="35sp">

        <TextView
            android:id="@+id/txtHeader9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tenant List"
            android:textSize="60sp"
            android:paddingTop="60sp"
            android:layout_centerHorizontal="true"/>

        <ImageView
            android:id="@+id/imgarrowBack9"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:src="@drawable/ic_back_arrow"/>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recycleViewItem9"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:layout_below="@+id/txtHeader9"
            android:paddingTop="10sp"
            android:layout_marginTop="20sp"/>

    </RelativeLayout>

</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>